    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Node.js Launch Program",
            "program": "${file}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Python: Current file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Run project",
            "type": "python",
            "request": "launch",
            "program": "compiler_run.py",
            "args": ["./test/test_js_files/quickSort.js"],
            "console": "integratedTerminal"
        }
    ]
}